USES mig_config;

VAR
   l_list                        TStringList;
   l_index                       number;
BEGIN
   l_list := GetFileList(GetSourceDir, '*.pll', false);
   for l_index := 0 to l_list.count-1 do
   begin
      logadd(rpad(l_list.strings[l_index], 50, ' ') + ' : ' + API_ModuleVersionString(l_list.strings[l_index], false));
   end;
   l_list.free;
END.
